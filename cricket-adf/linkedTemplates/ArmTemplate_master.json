{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cricket-adf"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_cricket_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_cricket_databricks'"
		},
		"ls_cricket_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_cricket_db'"
		},
		"ls_landing_zone_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_landing_zone'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cricketadls.dfs.core.windows.net/"
		},
		"ls_cricket_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Cricket-KV.vault.azure.net/"
		},
		"ls_cricksheet_download_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cricsheet.org/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cricket-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_cricket_databricks_accessToken": {
						"value": "[parameters('ls_cricket_databricks_accessToken')]"
					},
					"ls_cricket_db_connectionString": {
						"value": "[parameters('ls_cricket_db_connectionString')]"
					},
					"ls_landing_zone_connectionString": {
						"value": "[parameters('ls_landing_zone_connectionString')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_cricket_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_cricket_KV_properties_typeProperties_baseUrl')]"
					},
					"ls_cricksheet_download_properties_typeProperties_url": {
						"value": "[parameters('ls_cricksheet_download_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}